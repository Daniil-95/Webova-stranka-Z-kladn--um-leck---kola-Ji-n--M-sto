extends  ../_skeleton

//- Includes
include ../elements/_ui-quote-default

block variables
	- var activeIndex = 5
	- var pageName = 'Typography'

block title
	title=pageName

block page
	// Page Header
	include ../sections/_header-default-with-top-panel-var-2
	main
		+breadcrumbs({
			path: [['Pages', '#'], [pageName]],
			bgImageClass: 'breadcrumbs-01'
		})

		// Base typography
		section.section-sm.bg-white.text-sm-left
			.shell
				.range
					.cell-md-8
						ul.list-xl
							li
								h1 H1 Heading
								p Welcome to our wonderful world. We sincerely hope that each and every user entering our website will find exactly what he/she is looking for. With advanced features of activating account and new login widgets, you will definitely have a great experience of using our web page.
							li
								h2 H2 Heading
